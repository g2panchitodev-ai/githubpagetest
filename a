[32m+[m[32m    <script>[m
[32m+[m[32m        // Calcula los días de estadía y el costo total[m
[32m+[m[32m        function calcularEstadia() {[m
[32m+[m[32m            const fechaIngreso = new Date(document.getElementById('fecha_ingreso').value);[m
[32m+[m[32m            const fechaSalida = new Date(document.getElementById('fecha_salida').value);[m
[32m+[m[32m            const tarifaDia = parseFloat(document.getElementById('tarifa_dia').value);[m
[32m+[m
[32m+[m[32m            if (fechaIngreso && fechaSalida && tarifaDia) {[m
[32m+[m[32m                const diferencia = fechaSalida.getTime() - fechaIngreso.getTime();[m
[32m+[m[32m                const dias = Math.ceil(diferencia / (1000 * 3600 * 24));[m
[32m+[m[32m                document.getElementById('dias_estadia').value = dias;[m
[32m+[m[32m                document.getElementById('costo_total_estadia').value = (dias * tarifaDia).toFixed(2);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // Event listeners[m
[32m+[m[32m        document.getElementById('fecha_ingreso').addEventListener('change', calcularEstadia);[m
[32m+[m[32m        document.getElementById('fecha_salida').addEventListener('change', calcularEstadia);[m
[32m+[m[32m        document.getElementById('tarifa_dia').addEventListener('input', calcularEstadia);[m
[32m+[m
[32m+[m[32m        document.getElementById('vehicleForm').addEventListener('submit', function(e) {[m
[32m+[m[32m            e.preventDefault();[m
